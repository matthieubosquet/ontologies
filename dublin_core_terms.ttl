    rdfs:isDefinedBy <http://purl.org/dc/terms/> ;
    rdfs:label "DCMI Box"@en ;
    rdfs:seeAlso <https://www.dublincore.org/specifications/dublin-core/dcmi-box/> .

dc:DCMIType
    dc:issued "2000-07-11"^^<http://www.w3.org/2001/XMLSchema#date> ;
